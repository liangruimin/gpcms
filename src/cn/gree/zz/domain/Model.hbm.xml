<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.gree.zz.domain">

	<class name="Model" table="gree_model">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="model" column="model"></property>
		<property name="name" column="name"></property>
		<property name="series" column="series"></property>
		<property name="property" column="property"></property>

	</class>

</hibernate-mapping>
